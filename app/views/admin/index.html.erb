<h1>Admin Login</h1>
<%= button_to "Sign in with Google", "/auth/google_oauth2", method: :post, data: { turbo: false } %>
<% if flash[:alert] %>
  <script>
    window.alert("<%= j flash[:alert] %>");
  </script>
<% end %>
<% if flash[:notice] %>
  <div class="alert alert-info"><%= flash[:notice] %></div>
<% end %>